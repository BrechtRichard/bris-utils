	<!-- legal language for EDepot Modification -->
	<xs:simpleType name="eDepotLanguage" final="restriction" >
		<xs:restriction base="xs:string">
			<xs:enumeration value="fr" />
			<xs:enumeration value="nl" />
			<xs:enumeration value="de" />
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="localized" abstract="true">
		<xs:attribute name="language" type="edmda:eDepotLanguage" use="required"/>
	</xs:complexType>

	<xs:complexType name="localizedName">
		<xs:complexContent>
			<xs:extension base="edmda:localized">
				<xs:sequence>
					<xs:element name="value" type="xs:string"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="asIsHeadOfficeAddress">
		<xs:sequence>
			<xs:element name="number" type="xs:string" />
			<xs:element name="postbox" type="xs:string" minOccurs="0"/>
			<xs:element name="postalCode" type="xs:string" />
			<xs:element name="countryCode" type="xs:string" />

			<xs:element name="nisCode" type="xs:string" />
			<xs:element name="streetCode" type="xs:string" />

			<xs:element name="municipalityNames" type="edmda:localizedName" maxOccurs="3"/>
			<xs:element name="streetNames" type="edmda:localizedName" maxOccurs="3"/>

		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="headOfficeAddressChange">
		<xs:complexContent>
			<xs:extension base="edmda:asIsHeadOfficeAddress">
				<xs:sequence>
					<xs:element name="effectivityDate" type="xs:date" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="stoppedAddress">
		<xs:sequence>
			<xs:element name="effectivityDate" type="xs:date"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="stoppedGlobalDenomination">
		<xs:complexContent>
			<xs:extension base="edmda:localized">
				<xs:sequence>
					<xs:element name="effectivityDate" type="xs:date" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


