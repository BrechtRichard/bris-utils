<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           targetNamespace="http://fednot.be/2023/08/08/lta/anna/enterpriseDepositArchive"
           xmlns="http://fednot.be/2023/08/08/lta/anna/enterpriseDepositArchive"
           xmlns:common="http://fednot.be/2023/08/08/lta/anna/common">

    <xs:annotation>
        <xs:documentation xml:lang="en">
            Common data structures used in the Enterprise Creation and Modification
            Deposit archives.

            History of changes:
            - 08/08/2023 : updated namespaces + added the juridicalDistrictDivisionType
            - 08/05/2023 : formal on the documentTypes + added INITIAL_OR_COORDINATED_STATUTES type + removed annextype (string instead)
            - 02/03/2025 : juridicalDistrictType mandatory
            - 25/01/2023 : updated namespaces + added juridical districts needed for company deeds search.
            - 23/01/2023 : initial version.

        </xs:documentation>
    </xs:annotation>

    <xs:import namespace="http://fednot.be/2023/08/08/lta/anna/common"
               schemaLocation="CommonTypes.xsd" />

    <xs:simpleType name="actObjectCode">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                An act object code.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="50" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="legalFormCode">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                A legal form code.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="10" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="enterpriseDepositType" final="restriction">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                Enumeration to indicate the type of enterprise deposit: creation or modification.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="ENTERPRISE_CREATION">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                        An enterprise creation.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ENTERPRISE_MODIFICATION">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                        An enterprise modification.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>


